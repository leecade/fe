  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "coveralls": "^2.11.14",
    "cross-env": "^3.1.3",
    "espower": "^2.0.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-ava": "^0.14.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-newer": "^1.3.0",
    "gulp-optimize-js": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-standard": "^8.0.2",
    "gulp-util": "^3.0.7",
    "gzip-size": "^3.0.0",
    "nyc": "^8.4.0",
    "power-assert": "^1.4.1",
    "pre-commit": "^1.1.3",
    "snazzy": "^5.0.0",
    "standard": "^8.5.0"
  },
  "keywords": [
    "fe"
  ],
  "engines": {
    "npm": "3.x"
  },
  "config": {
    "unsafe-perm": true
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "fe": "./bin/fe.js"
  },
  "scripts": {
    "test": "npm run lint && npm run build && nyc --all ava",
    "test:win": "npm run lint && npm run build && nyc --all ava",
    "test:watch": "ava --watch",
    "coverage": "nyc report",
    "coverage:report": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf lib",
    "babel:build": "babel src --out-dir lib",
    "babel:watch": "babel src --out-dir lib --watch",
    "watch": "gulp watch",
    "build": "cross-env BABEL_ENV=production gulp",
    "dev": "npm run build && npm run watch",
    "start": "npm run build",
    "lint": "standard",
    "release": "./scripts/release.sh",
    "update": "updtr",
    "precommit": "git diff --name-only --cached --relative | grep '\\.jsx\\?$' | xargs standard | snazzy; if [ $? -ne 0 ]; then exit 1; fi"
  },
  "pre-commit": {
    "run": [
      "precommit"
    ],
    "silent": true
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "fetch",
      "alert"
    ],
    "ignore": [
      "lib/**/*.js",
      "*.test.js",
      "src/plugins/**/*.js",
      "example/build/**/*.js",
      "node_modules/**/*.js"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/helpers/**/*.js"
    ],
    "source": [
      "test/helpers/**/*.js"
    ],
    "concurrency": 50,
    "failFast": false,
    "powerAssert": false,
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.{js|jsx}",
      "bin/**/*.{js|jsx}"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.test.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-core/register"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": ".nyc_output"
  },
  "license": "MIT"
}
